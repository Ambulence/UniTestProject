{
  "name": "unitestproject",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm-run-all --parallel frontend backend",
    "develop": "npm-run-all --parallel frontend backend-dev",
    "ts-build": "cd uni-back-end && npm run build",
    "frontend": "cd front-end && npm start",
    "backend": "cd uni-back-end && npm start",
    "backend-dev": "cd uni-back-end && npm run develop",
    "install": "npm run i-front && npm run i-back",
    "i-front": "cd front-end && npm install",
    "i-back": "cd uni-back-end && npm install"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
